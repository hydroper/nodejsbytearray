{
    "compilerOptions": {
        "moduleResolution": "node",
        "lib": ["es2015"],
        "target": "es2015",
        "types": ["@types/node"],
        "baseUrl": ".",
        "noFallthroughCasesInSwitch": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "noImplicitOverride": true,
        "noImplicitThis": true
    }
}